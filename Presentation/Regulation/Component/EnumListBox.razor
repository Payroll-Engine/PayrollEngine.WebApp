@typeparam T

<MudSelect T="T" 
           Label="@Item.GetItemLabel(Field, Localizer)"
           HelperText="@Item.GetItemHelp(Field)"
           Value="@Value" ValueChanged="ValueChangedAsync"
           ReadOnly="@Item.IsReadOnlyField(Field)"
           Required="@Field.Required"
           RequiredError="@Field.RequiredError">
    @foreach (var value in GetEnumValues())
    {
        <MudSelectItem T="T" Value="@value.Item1">@value.Item2</MudSelectItem>
    }
</MudSelect>

